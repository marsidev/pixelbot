    "@babel/traverse": "^7.28.4"
  },
  "devDependencies": {
    "@babel/types": "^7.28.4",
    "@biomejs/biome": "2.2.3",
    "bumpp": "^10.2.3",
    "pnpm": "10.15.1",
    "rolldown": "1.0.0-beta.34",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  }
}
